{
    "name": "kianisanaullah/social_media",
    "description": "A social media tool kit using Laravel and Livewire",
    "keywords": [
        "laravel",
        "laravel-package",
        "social-media",
        "livewire"
    ],
    "homepage": "https://github.com/kianisanaullah/social_media",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kianisanaullah\\SocialMedia\\": "src/"
        },
        "files": [
            "src"
        ]
    },
    "authors": [
        {
            "name": "Sanaullah Kiani",
            "email": "kianisanaullah@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {

    },
    "autoload-dev": {

    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "laravel": {
            "providers": [
                "Kianisanaullah\\SocialMedia\\ServiceProvider"
            ]
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kianisanaullah\\SocialMedia\\ServiceProvider"
            ],
            "aliases": {
                "SocialMedia": "Kianisanaullah\\SocialMedia\\ServiceProvider"
            }
        }
    }
}
